	<select id="listarExportacionTerrestrexFiltro" resultMap="exportacionTerrestreMap" parameterType="String">	  
		SELECT 
			<include refid="Base_Column_List" />
		FROM COMEXSVI_EXPORTACION_01
		WHERE ID IS NOT NULL
		AND IDTIPOTRANSPORTE = 2
		<if test="codigos.length > 0">
			AND CODIGO IN 
			<foreach collection="codigos" item="item" open="(" close=")" separator=",">
			 #{item}
			</foreach>	
		</if>
		<if test="folio != null and folio != ''">
			AND FOLIOFACTURA LIKE #{folio}
		</if>
		<if test="clientes.length > 0">
			AND CODIGOSAPCLIENTE IN 
			<foreach collection="clientes" item="item" open="(" close=")" separator=",">
			 #{item}
			</foreach>	
		</if>
		<if test="fechaInicio != null and fechaInicio != '' and fechaFin != null and fechaFin != ''">      
	       AND ( FECHACREACION BETWEEN TO_DATE (#{fechaInicio}, 'yyyy-mm-dd') AND 
	        TO_DATE(#{fechaFin}, 'yyyy-mm-dd') + INTERVAL '1' DAY)
	   </if>
		<if test="pagos.length > 0">
			AND IDCONDICIONPAGO IN 
			<foreach collection="pagos" item="item" open="(" close=")" separator=",">
			 #{item}
			</foreach>	
		</if>
		AND ROWNUM <![CDATA[ <= ]]> 50
		ORDER BY FECHACREACION DESC
	</select>
	<select id="obtenerExportacionTerrestrexId" resultMap="exportacionTerrestreMap" parameterType="int">	  
	SELECT 
	<include refid="Base_Column_List" />
	FROM 
	COMEXSVI_EXPORTACION_01
	WHERE ID = #{idDocumento}
	AND ROWNUM <![CDATA[ <= ]]> 1
	</select>
	<select id="obtenerExportacionTerrestrexFactura" resultMap="exportacionTerrestreMap">	  
	SELECT EXPON_ID AS ID
	FROM COMEXST_EXPORTACION
	WHERE EXPON_IDFACTURA=#{idFactura} 
	AND EXPON_IDESTADODOCUMENTO=#{idEstadoDocumento}
	AND ROWNUM <![CDATA[ <= ]]> 1
	</select>
	<select id="listarPosicionxIdExportacion" resultMap="exportacionTerrestrePosicionMap">
		SELECT * 
		FROM COMEXSVI_EXPORTACION_04_TERRESTREPOSICION
		WHERE IDEXPORTACION = #{idExportacion}
	</select>
